{
  "name": "@paymentiq-backoffice-2/frontend",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_ENVIRONMENT=DEV_HOSTED react-scripts start",
    "start:local": "REACT_APP_ENVIRONMENT=DEV react-scripts start",
    "start:unix": "react-scripts start",
    "start:windows": "react-scripts start",
    "build": "REACT_APP_ENVIRONMENT=TEST react-scripts build",
    "build:prod": "REACT_APP_ENVIRONMENT=PRODUCTION react-scripts build",
    "test": "react-scripts test",
    "test:once": "yarn test --watchAll=false",
    "publish": "node build/deploy.js --",
    "eject": "react-scripts eject",
    "format": "prettier --write ./src/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint ./src --ext .ts --ext .js --ext .tsx --fix",
    "lint:pre-commit": "npm run lint:fix",
    "isready": "npm run format && npm run lint && npm run build",
    "generate-pages:all": "node ./bin/generate-pages/generate-index.js --all-pages",
    "generate-test-pages:all": "node ./bin/generate-pages/generate-index.js --all-test-pages",
    "generate-pages:single": "node ./bin/generate-pages/generate-index.js --single-page"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.2.0",
    "@paymentiq-backoffice-2/shared": "1.0.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/styled-components": "^5.1.9",
    "aws-sdk": "^2.903.0",
    "dotenv": "^9.0.1",
    "history": "4.10.1",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.21",
    "msw": "^0.34.0",
    "node-sass": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.32.12",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "styled-components": "^5.3.0",
    "use-between": "^0.0.18",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.0",
    "@types/lodash": "^4.14.172",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "inquirer": "^8.1.1",
    "prettier": "2.3.0",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
